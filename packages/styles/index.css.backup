/* ---------------------------------------------------------
   UTILITIES & VARIABLES (Refactored)
   --------------------------------------------------------- */
/* Colors - Refactored from variables to classes */
.color-primary {
  color: #732882 !important;
}
.color-primary-dark {
  color: #5a1f6b !important;
}
.color-secondary {
  color: #daa23e !important;
}
.color-secondary-dark {
  color: #b8862d !important;
}

.color-black {
  color: #1a1a1a !important;
}
.color-white {
  color: #ffffff !important;
}
.color-light {
  color: #ffffff !important;
}
.color-grey-light {
  color: #f8f9fa !important;
}
.color-grey {
  color: #ccc !important;
}
.color-text-dark {
  color: #333 !important;
}

/* Backgrounds */
.bg-primary {
  background-color: #732882 !important;
}
.bg-secondary {
  background-color: #daa23e !important;
}
.bg-black {
  background-color: #1a1a1a !important;
}
.bg-light {
  background-color: #ffffff !important;
}
.bg-cream {
  background-color: #ffe3b852 !important;
}
.bg-gradient-main {
  background-image: linear-gradient(to right, #fffcf6, #fff2da);
}

.background-primary {
  background: #732882 !important;
}
.background-secondary {
  background: #daa23e !important;
}
.background-black {
  background: #1a1a1a !important;
}
.background-light {
  background: #ffffff !important;
}

/* Font Weights */
.fw-light {
  font-weight: 300;
}
.fw-regular {
  font-weight: 400;
}
.fw-medium {
  font-weight: 500;
}
.fw-semi-bold {
  font-weight: 600;
}
.fw-bold {
  font-weight: 700;
}
.fw-extra-bold {
  font-weight: 900;
}

/* Font Sizes */
.p-xl {
  font-size: 25px;
}
.p-lg {
  font-size: 20px;
}
.p-md {
  font-size: 18px;
  line-height: 24px;
}
.p-sm {
  font-size: 16px;
}
.p-xs {
  font-size: 14px;
}
.p-xxs {
  font-size: 12px;
}

/* Utils */
.py-50 {
  padding: 50px 0;
}
.min-vh-80 {
  min-height: 80vh;
}
.heading {
  font-size: 32px;
  font-weight: 600;
  margin: 0;
}
.vert-move {
  transition: all 0.4s ease;
}
.vert-move:hover {
  transform: translateY(-5px);
}

.text-sand {
  color: #daa23e;
  font-style: inherit;
  font-weight: inherit;
}
.bg-sand {
  background-color: #732882;
}
.text-purple {
  color: #732882;
}

/* ---------------------------------------------------------
   GLOBAL COMPONENTS
   --------------------------------------------------------- */
.btn-global {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 10px 24px;
  border-radius: 50px;
  font-weight: 500;
  cursor: pointer;
  transition: all 0.3s ease;
  border: 1px solid transparent;
  text-decoration: none;
  font-size: 16px;
  gap: 8px;
}

.btn-primary {
  background-color: #732882;
  color: #ffffff;
  border-color: #732882;
}

.btn-primary:hover {
  background-color: #5a1f6b;
  border-color: #5a1f6b;
  color: #ffffff;
  transform: translateY(-2px);
}

.btn-secondary {
  background-color: #daa23e;
  color: #ffffff;
  border-color: #daa23e;
}

.btn-secondary:hover {
  background-color: #b8862d;
  border-color: #b8862d;
  color: #ffffff;
  transform: translateY(-2px);
}

.btn-outline-primary {
  background-color: transparent;
  color: #732882;
  border-color: #732882;
}

.btn-outline-primary:hover {
  background-color: #732882;
  color: #ffffff;
}

.btn-outline-secondary {
  background-color: transparent;
  color: #daa23e;
  border-color: #daa23e;
}

.btn-outline-secondary:hover {
  background-color: #daa23e;
  color: #ffffff;
}

/* Titles */
.title-xl {
  font-size: 45px;
  font-weight: 700;
  color: #732882;
  line-height: 1.2;
}

.title-lg {
  font-size: 32px;
  font-weight: 600;
  color: #732882;
  margin-bottom: 20px;
}

.title-md {
  font-size: 24px;
  font-weight: 600;
  color: #daa23e;
}

.title-sm {
  font-size: 18px;
  font-weight: 600;
  color: #333;
}

/* Legacy Utility Compat */
.bg-primary-main {
  background-color: #732882;
}
.bg-secondary-main {
  background-color: #daa23e;
}
.bg-cream-light {
  background-color: #ffe3b852;
}

/* ---------------------------------------------------------
   HEADER STYLES
   --------------------------------------------------------- */
.logo {
  width: 170px;
  background: #fff;
  border-radius: 15px;
  padding: 9px;
  box-shadow: 0 4px 6px 3px #0000000d;
  margin-top: -60px;
}

.top-head {
  padding: 7px 0;
  background-image: linear-gradient(to right, #b8862d, #daa23e);
}

.top-text {
  margin: 0;
  font-size: 18px;
  color: #fff;
}

.top-menu-main li a {
  color: #000 !important;
  font-size: 16px;
  padding: 9px 15px !important;
}

.top-menu-main li a:hover {
  color: #732882 !important;
}

.btn-link {
  background: #732882;
  color: #ffffff;
  width: 100%;
  padding: 14px 13px;
  border-radius: 100px;
  text-decoration: none;
  display: block;
  text-align: center;
}

.btn-link img {
  margin-right: 5px;
  filter: brightness(0) invert(1);
}

.btn-link:hover {
  color: #fff;
  background: #186593;
}

ul.dropdown-menu {
  padding: 0;
}

.top-menu-main li ul.dropdown-menu li a {
  transition: 0.6s;
  border-bottom: solid 1px rgba(218, 162, 62, 0.45);
}

.top-menu-main li ul.dropdown-menu li a:hover {
  background: #732882;
  color: #ffffff !important;
}

a.nav-link {
  color: #1a1a1a;
  padding: 9px 15px !important;
}

a.nav-link :hover {
  color: #702b83 !important;
  padding: 9px 15px !important;
}

.nav-link:focus,
.nav-link:hover {
  color: #732882;
}

.lang-dropdown {
  position: relative;
  display: inline-block;
  width: 100%;
}

.lang-toggle {
  background: #030303;
  color: #ffffff;
  padding: 8px 13px;
  border: none;
  border-radius: 20px;
  cursor: pointer;
  font-weight: 600;
  width: 100%;
  font-size: 14px;
}

.lang-menu {
  display: none;
  position: absolute;
  top: 100%;
  left: 0;
  background: white;
  border-radius: 8px;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
  min-width: 120px;
  z-index: 10;
}

.lang-menu a {
  display: block;
  padding: 10px 15px;
  color: #333;
  text-decoration: none;
}

.lang-menu a:hover {
  background: #f1f1f1;
  border-radius: 8px;
}

.lang-dropdown:hover .lang-menu {
  display: block;
}

.cart-top {
  color: #fff;
  font-size: 14px;
  position: relative;
}

.cart-top:hover {
  color: #fff;
}

.value {
  display: block;
  position: absolute;
  left: 5px;
  top: -12px;
  background: #fd0c38;
  width: 17px;
  height: 17px;
  border-radius: 100px;
  text-align: center;
  font-size: 10px;
}

.account-dropdown {
  position: relative;
  display: inline-block;
}

.account-btn {
  background: #030303;
  color: #ffffff;
  padding: 8px 16px;
  border: none;
  cursor: pointer;
  font-weight: 500;
  display: flex;
  align-items: center;
  gap: 6px;
  border-radius: 20px;
}

.account-menu {
  display: none;
  position: absolute;
  top: 110%;
  left: 0;
  background: white;
  border-radius: 10px;
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.15);
  min-width: 140px;
  z-index: 10;
}

.account-menu a {
  display: block;
  padding: 10px 15px;
  color: #333;
  text-decoration: none;
}

.account-menu a:hover {
  background: #f1f1f1;
}

.account-dropdown.show .account-menu {
  display: block;
}

/* ---------------------------------------------------------
   MAIN LAYOUT & SECTIONS
   --------------------------------------------------------- */
body {
  font-family: "Outfit", sans-serif;
  background-image: url("/images/white-background.png");
  background-size: cover;
  background-repeat: no-repeat;
  background-attachment: fixed;
}

img {
  max-width: 100%;
}

ul {
  padding: 0;
  margin: 0;
}

.align {
  align-items: center;
}

a {
  text-decoration: none;
  color: white;
}

.navbar-toggler {
  background: #ffd281;
}

/* Banner Part */
.banner-part {
  background-image: url("/images/back-over.jpg");
  background-size: cover;
  background-repeat: no-repeat;
}

.overlay-hero {
  background: #000000c4;
  padding: 45px 0;
}

.banner-part h1 {
  font-size: 45px;
  color: #fff;
  font-weight: 600;
  letter-spacing: 1px;
}

.card-title {
  background: #7328823d; /* Converted from legacy */
  width: fit-content;
  padding: 8px 15px;
  color: #fff;
  border-left: solid #daa23e;
  font-size: 23px;
}

.banner-part p {
  color: #fff;
  font-size: 16px;
}

.list-check li {
  list-style: none;
  position: relative;
  color: #fff;
  font-size: 16px;
  padding: 7px 0px;
}

.list-check li i {
  color: #ffffff;
  margin-right: 5px;
  background: #d08830;
  padding: 4px;
  border-radius: 100px;
  width: 25px;
  height: 25px;
  text-align: center;
}

.wfc {
  width: fit-content;
  padding: 15px 23px;
  letter-spacing: 1px;
}

.card-hero {
  background: #73288217;
  border-radius: 15px;
  padding: 9px;
  box-shadow: 0 4px 6px 3px #0000000d;
  border: solid 1px #daa23e;
  margin: 13px 0;
  text-align: center;
  min-height: 216px;
}

.card-hero h5 {
  color: #daa23e;
  font-size: 22px;
}

.card-hero p {
  font-size: 16px;
}

.card-hero img {
  width: 77px;
  margin: 11px 0;
}

/* Astrologer List */
.astrologer-list h2 {
  text-align: center;
  font-size: 32px;
  font-weight: 600;
  margin-bottom: 20px;
}

.astro-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 20px;
}

.astro-card {
  background: #fff;
  border-radius: 12px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
  padding: 14px;
  text-align: center;
  transition: transform 0.3s ease;
  background-image: linear-gradient(to right, #fffcf6, #fff2da);
  border: solid 1px rgba(218, 162, 62, 0.27); /* #daa23e45 */
}

.astro-card:hover {
  transform: translateY(-5px);
}

.rating-star {
  color: #daa23e;
  font-size: 22px;
}

.astro-name {
  font-size: 18px;
  font-weight: 600;
  margin-bottom: 5px;
  color: #732882;
}

.vid-part {
  position: relative;
  width: 150px;
  margin: 0 auto;
}

.play-vid {
  position: absolute;
  bottom: 6px;
  right: 18px;
  font-size: 19px;
  background: #daa23e;
  width: 35px;
  height: 35px;
  padding-top: 4px;
  border-radius: 100px;
  color: #fff;
  cursor: pointer;
}

.modal-title-astro-about {
  font-size: 25px;
  padding: 0;
  margin: 0;
  color: #732882;
  font-weight: 700;
}

.astro-card .astro-profile-img {
  width: 120px;
  height: 120px;
  object-fit: cover;
  border-radius: 50%;
  margin-bottom: 8px;
  border: solid 1px #daa23e;
}

button.btn-close {
  opacity: 1;
  z-index: 0;
  border-radius: 100px;
  color: #fff;
  font-size: 22px;
  padding: 10px !important;
  line-height: 1;
}

.modal-content {
  background-image: url("/images/white-background.png") !important;
  background-size: cover;
  background-repeat: no-repeat;
}

.checkmark {
  position: absolute;
  top: 0;
  right: 0;
}

.astro-tags {
  display: inline-block;
  background: #daa23e;
  color: #fff;
  font-size: 14px;
  padding: 4px 10px;
  border-radius: 20px;
  margin-bottom: 0px;
  margin-top: 5px;
}

.astro-info {
  font-size: 16px;
  margin: 8px 0;
  color: #333;
}

.astro-actions {
  display: flex;
  justify-content: center;
  gap: 10px;
  margin-top: 15px;
}

.astro-actions button {
  flex: 1;
  padding: 8px 12px;
  border-radius: 25px;
  border: 1px solid #732882;
  background: #fff0;
  color: #732882;
  cursor: pointer;
  font-size: 14px;
  transition: all 0.3s ease;
}

.astro-actions button.call {
  border-color: #1a1a1a;
  color: #1a1a1a;
}

.astro-actions button:hover {
  background: #732882;
  color: #ffffff;
}

.astro-actions button.call:hover {
  background: #1a1a1a;
  color: #ffffff;
}

.view-all {
  text-align: center;
  margin-top: 40px;
}

/* Talk to Astrologer */
.talk-to-astrologer-banner {
  margin-bottom: 30px;
}

.banner-image-placeholder {
  background: linear-gradient(135deg, #732882 0%, #daa23e 100%);
  border-radius: 15px;
  padding: 40px 20px;
  text-align: center;
  min-height: 150px;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
  position: relative;
  overflow: hidden;
}

.banner-image-placeholder::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: url("/images/pattern.png");
  opacity: 0.1;
  pointer-events: none;
}

.banner-text {
  color: #fff;
  font-size: 36px;
  font-weight: 700;
  margin: 0;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.2);
  position: relative;
  z-index: 1;
}

/* Search Box & Filters */
.search-filter-row {
  display: flex;
  align-items: center;
  gap: 12px;
  margin-bottom: 30px;
}

.search-wrapper {
  flex: 1;
}

.search-box {
  display: flex;
  justify-content: flex-start;
  margin-bottom: 0;
}

.search-box input {
  flex: 1;
  max-width: none;
  padding: 12px 20px;
  border: 1px solid #ccc;
  border-radius: 30px 0 0 30px;
  outline: none;
  font-size: 16px;
}

.search-box .search-icon-btn {
  padding: 12px 20px;
  background: #732882;
  color: #fff;
  border: none;
  border-radius: 0 30px 30px 0;
  cursor: pointer;
  font-size: 18px;
  width: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: background 0.3s ease;
}

.search-box .search-icon-btn:hover {
  background: #5a1f6b;
}

.btn-filter,
.btn-sort {
  background: #732882;
  color: white;
  border: none;
  border-radius: 30px;
  padding: 12px 24px;
  font-weight: 500;
  font-family: inherit;
  cursor: pointer;
  transition: background 0.15s ease;
  display: flex;
  align-items: center;
  gap: 8px;
  white-space: nowrap;
}

.btn-filter:hover,
.btn-sort:hover {
  background: #5a1f6b;
}

/* Specialization Carousel */
.specialization-carousel-container {
  position: relative;
  margin-bottom: 40px;
  padding: 0 50px;
}

.specialization-carousel {
  display: flex;
  gap: 12px;
  overflow-x: auto;
  scroll-behavior: smooth;
  padding: 10px 0;
  scrollbar-width: none;
  -ms-overflow-style: none;
}

.specialization-carousel::-webkit-scrollbar {
  display: none;
}

.specialization-btn {
  background: #fff;
  border: 2px solid #daa23e;
  border-radius: 25px;
  padding: 10px 20px;
  display: flex;
  align-items: center;
  gap: 8px;
  cursor: pointer;
  transition: all 0.3s ease;
  white-space: nowrap;
  font-size: 15px;
  font-weight: 500;
  color: #333;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.08);
}

.specialization-btn:hover {
  background: #732882;
  color: #fff;
  border-color: #732882;
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(115, 40, 130, 0.3);
}

.specialization-btn.active {
  background: #732882;
  color: #fff;
  border-color: #732882;
  box-shadow: 0 4px 12px rgba(115, 40, 130, 0.4);
}

.specialization-btn.active:hover {
  background: #5a1f6b;
  border-color: #5a1f6b;
}

.spec-icon {
  font-size: 18px;
}

.spec-name {
  font-size: 14px;
}

.carousel-arrow {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background: #732882;
  color: white;
  border: none;
  border-radius: 50%;
  width: 40px;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: all 0.3s ease;
  z-index: 10;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
}

.carousel-arrow:hover {
  background: #5a1f6b;
  transform: translateY(-50%) scale(1.1);
}

.carousel-arrow-left {
  left: 0;
}

.carousel-arrow-right {
  right: 0;
}

/* Custom Buttons */
.orderNowbtn {
  border: none;
  border-radius: 50px;
  padding: 7px 20px;
  background-color: #732882;
}

.orderNowbtn:hover {
  border: none;
  border-radius: 50px;
  padding: 7px 20px;
  background-color: #5a1f6b;
}

/* Horoscopes Section */
.horoscopes-container {
  background: #ffe3b852;
  padding: 50px 0;
}

.horoscopes-container h2 {
  font-size: 32px;
  font-weight: 600;
  margin: 0;
}

.horoscopes-items {
  background-color: #ffffff;
  overflow: hidden;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
  margin: 10px 0px 20px 0px;
  border: solid 1px rgba(218, 162, 62, 0.45);
  text-align: center;
  padding: 20px;
  border-radius: 10px;
  transition: all 0.4s ease;
  color: #1a1a1a;
}

.horoscopes-items h3 {
  font-size: 18px;
  padding-top: 10px;
}

.horoscopes-items p {
  font-size: 15px;
  margin-bottom: 0;
}

.horoscopes-items img {
  width: 80px;
  height: 80px;
  transition: transform 0.4s ease;
}

.horoscopes-items h3,
.horoscopes-items p {
  transition: color 0.4s ease;
  margin-bottom: 0;
}

.horoscopes-items:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
}

.horoscopes-items:hover img {
  transform: scale(1.1);
}

/* ---------------------------------------------------------
   SIGN IN PART
   --------------------------------------------------------- */
.signin-part {
  background: linear-gradient(to right, #fffcf6, #fff2da);
  width: 100%;
  height: auto;
  padding-top: 30px;
  padding-bottom: 120px;
  margin-bottom: -100px;
}

.banner-data {
  padding-top: 2rem;
}

.form-data {
  margin: 5rem 0px 0px 10rem;
  border: 2px solid rgba(218, 162, 62, 0.45);
  border-radius: 12px;
  background-color: white;
  padding: 20px;
}

.sign-form-heading {
  display: flex;
  justify-content: space-between;
  align-items: center;
  gap: 5rem;
}

.sign-up {
  text-decoration: none;
  color: #daa23e;
}

.social-button {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 10px;
  background-color: #e9f1ff;
  color: #1a1a1a;
  width: 50%;
  padding: 5px 20px;
  border-radius: 12px;
  cursor: pointer;
}

.links-images {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 10px;
}

.social-button2 {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 10px;
  background: #00a2ff8e;
  border-radius: 12px;
  padding: 7px 30px;
  cursor: pointer;
}

.form-email input {
  width: 90%;
  padding: 8px 10px;
  border-radius: 15px;
  border: none;
  border: 2px solid rgba(218, 162, 62, 0.45);
  margin-top: 5px;
}

.form-email input:focus {
  border-color: #732882;
  outline: none;
  box-shadow: 0 0 5px rgba(115, 40, 130, 0.3);
}

.form-password input {
  width: 90%;
  padding: 8px 10px;
  border-radius: 15px;
  border: none;
  border: 2px solid rgba(218, 162, 62, 0.45);
  margin-top: 5px;
}

.form-password input:focus {
  border-color: #732882;
  outline: none;
  box-shadow: 0 0 5px rgba(115, 40, 130, 0.3);
}

.form-password {
  flex-direction: column;
}

.form-password label,
.form-email label {
  font-size: 14px;
}

.forget-password {
  display: flex;
  justify-content: flex-end;
  text-decoration: none;
  align-items: end;
  padding-top: 10px;
  color: #daa23e;
  font-weight: 500;
