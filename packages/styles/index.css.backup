.product-btn-luxury {
  background: linear-gradient(135deg, #732882 0%, #a855f7 100%);
  color: #fff;
  border: 2px solid transparent;
  padding: 14px 30px;
  border-radius: 30px;
  font-weight: 700;
  width: 100%;
  cursor: pointer;
  transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
  position: relative;
  overflow: hidden;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.product-btn-luxury::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);
  transition: left 0.5s ease;
}

.product-btn-luxury:hover::before {
  left: 100%;
}

.product-btn-luxury:hover {
  transform: translateY(-3px);
  box-shadow: 0 10px 30px rgba(115, 40, 130, 0.5);
  border-color: #daa23e;
}

/* Modal Enhancements */
.modal-content-enhanced .modal-header {
  background: linear-gradient(135deg, #732882 0%, #a855f7 100%);
  color: #fff;
  border: none;
  padding: 20px 30px;
}

.modal-content-enhanced .modal-title {
  font-weight: 700;
  font-size: 24px;
}

.modal-content-enhanced .btn-close {
  filter: brightness(0) invert(1);
}

